            },
        "RAK3172_BREAKOUT": {
            "stm32wl-lora-driver.rf_switch_config": 2,
            "stm32wl-lora-driver.crystal_select": 0,
            "stm32wl-lora-driver.debug_invert": 1,
            "stm32wl-lora-driver.debug_rx": "LED1",
            "stm32wl-lora-driver.debug_tx": "LED2"
        }
